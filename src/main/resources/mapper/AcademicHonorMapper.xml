<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qingchun.travelloan.mapper.AcademicHonorMapper">
    
    <select id="selectByUserId" resultType="com.qingchun.travelloan.entity.AcademicHonor">
        SELECT * FROM academic_honor
        WHERE user_id = #{userId}
        ORDER BY created_at DESC
    </select>
    
</mapper>
